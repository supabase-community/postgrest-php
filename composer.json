{
    "name": "supabase/postgrest-php",
    "type": "library",
    "description": "Supabase PostgREST client for PHP",
    "keywords": ["supabase","postgrest", "postgrest-php"],
    "homepage": "https://github.com/supabase-community/postgrest-php",
    "autoload": {
        "psr-4": {
            "Supabase\\Postgrest\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Supabase\\Postgrest\\Test\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "jordan",
            "email": "jordan@ascensionengineer.com"
        },
        {
            "name": "norm",
            "email": "norm@zerocopylabs.com"
        },
        {
            "name": "Adolfo",
            "email": "adolfo@zerocopylabs.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "guzzlehttp/guzzle": "7.5",
        "vlucas/phpdotenv": "5.5",
        "spatie/url": "2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "9.6",
        "allure-framework/allure-phpunit": "1.5"
    }
}
